{
  "name": "@zenorm/generate",
  "description": "Easy ORM, easy query. easy typing! Auto generate typescript declaration.",
  "version": "1.0.0",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/yefei/zenorm-generate",
  "author": "YeFei <316606233@qq.com>",
  "license": "MIT",
  "bin": {
    "zenorm-generate": "./dist/bin/zenorm-generate.js"
  },
  "files": [
    "docs",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist docs && tsc && npm run docs",
    "prepublishOnly": "npm run build",
    "docs": "typedoc --plugin typedoc-plugin-markdown --disableSources src/types.ts",
    "gen": "cd test && ts-node ../src/bin/zenorm-generate.ts config.json"
  },
  "keywords": [
    "mysql",
    "query",
    "model",
    "types",
    "generate",
    "database"
  ],
  "devDependencies": {
    "@types/node": "^12.20.41",
    "@zenorm/generate-mysql": "^1.2.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.28",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "pascal-case": "^3.1.2",
    "snake-case": "^3.0.4"
  }
}
